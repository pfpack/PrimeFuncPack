	<RootNamespace>System</RootNamespace>
	<AssemblyName>PrimeFuncPack.Core.Func</AssemblyName>
	<Version>1.1.1-rc.1.0.0</Version>
  </PropertyGroup>

  <ItemGroup>
  	<None Include="..\..\..\LICENSE">
	  <Pack>True</Pack>
	  <PackagePath></PackagePath>
	</None>
  </ItemGroup>

</Project>
